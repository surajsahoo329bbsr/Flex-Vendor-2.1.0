<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.NoActionBar" > <!-- removes action bar from all activites of app -->
    <!-- Customize your theme here. --> <!-- Selectable removes action bar -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:fontFamily">@font/prod_sans_medium</item>
    </style>


    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlActivated">#4FC2F8</item>
        <item name="colorControlNormal">#4D4B4B</item>
    </style>
</resources>